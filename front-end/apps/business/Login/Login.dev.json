{
  "paths": {
    "login": "apps/business/login/login.config",
    "loginController": "apps/business/login/controllers/login.controller",
    "logoutController": "apps/business/login/controllers/logout.controller",
    "angular-toastr": "bower_components/angular-toastr/dist/angular-toastr.tpls.min"
  },


  "dependencies": {
    "loginController": {
      "deps": [ "login" ]
    },
    "logoutController": {
      "deps": [ "login" ]
    },
    "login": {
      "deps": [ "angular-toastr" ]
    },
    "angular-toastr": {
      "deps": [ "angular_animate", "angular_sanitize" ]
    }
  },


  "routes": {
    "moduleName": "login",
    "states": [
      {
        "name": "login",
        "url": "/login",
        "templateUrl": "apps/business/login/views/login.view.html",
        "controller": "loginController"
      },
      {
        "name": "logout",
        "url": "/logout",
        "templateUrl": "apps/business/login/views/logout.view.html",
        "controller": "logoutController"
      }
    ]
  }

}

